    <div class="container-fluid" id="fh5co-image-grid">
      <div class="grid">
        <div class="grid-sizer"></div>

        <% files.forEach(function(file) { %>
        <div class="grid-item item animate-box" data-animate-effect="fadeIn">
          <a href="images/<%= file %>" class="image-popup" title="<%= file %>">
            <div class="img-wrap">
              <img src="images/<%= file %>" alt="" class="img-responsive">
            </div>
            <div class="text-wrap">
              <div class="text-inner popup">
                <div>
                  <h2><%= file %></h2>
                </div>
              </div>
            </div>
          </a>
        </div>
        <% }); %>

